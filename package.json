{
  "name": "react-laravel-starter",
  "version": "0.0.1",
  "description": "Start project for using React with Laravel",
  "main": "index.js",
  "repository": "git@github.com:awylie199/react-laravel-starter.git",
  "author": "Alexander Wylie <awylie199@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "export NODE_PATH=. && npm run build && forever -c babel-node resources/assets/hmr.js",
    "build": "npm run build:server && npm run build:client",
    "build:server": "webpack --config webpack.server.js --progress --display-error-details",
    "build:client": "webpack --config webpack.client.js --progress --display-error-details",
    "mocha:test": "mocha-webpack --webpack-config webpack.test.js \"resources/assets/**/*.spec.js\"",
    "mocha:test:watch": "mocha-webpack --webpack-config webpack.test.js \"resources/assets/**/*.spec.js\" --watch",
    "test": "npm run mocha:test",
    "test:watch": "npm run mocha:test:watch",
    "lint": "eslint resources/assets -c .eslintrc",
    "lint:fix": "eslint resources/assets -c .eslintrc --fix"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "normalize-scss": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-form": "^6.8.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "remotedev-serialize": "^0.1.0",
    "styled-components": "^2.0.1"
  },
  "devDependencies": {
